{%- comment -%}
  Usage:
  - Auto-detects vendor from product.vendor, or override via include parameter `vendor: 'sika'` etc.
  - Supported vendor keys (slugified):
    sockwell, sika, pro-compression, jockey, genuine-grip
  - Renders only vendor-specific tables with internal segmentation (men/women/kids/unisex) where applicable.
{%- endcomment -%}

{%- assign chart_key = chart_id | default: section.id | default: 'chart' -%}
{%- assign vendor_source = vendor | default: product.vendor | default: '' -%}
{%- assign vendor_key = vendor_source | downcase | strip | replace: ' & ', ' and ' | replace: '&', 'and' | replace: '.', '' | replace: ',', '' | replace: '/', '-' | replace: '  ', ' ' | replace: ' ', '-' -%}

{%- assign product_title_lower = product.title | downcase -%}
{%- assign product_type_lower = product.type | downcase -%}

{%- assign is_coat = false -%}
{%- assign is_jacket = false -%}
{%- assign is_pants = false -%}
{%- assign is_shirt = false -%}
{%- assign is_headwear = false -%}
{%- assign is_footwear = false -%}

{%- if product_title_lower contains 'chef coat' or product_title_lower contains 'chef jacket' or product_type_lower contains 'chef coat' -%}
  {%- assign is_coat = true -%}
{%- endif -%}

{%- if product_title_lower contains 'jacket' or product_type_lower contains 'jacket' -%}
  {%- assign is_jacket = true -%}
{%- endif -%}

{%- if product_title_lower contains 'pant' or product_type_lower contains 'pant' -%}
  {%- assign is_pants = true -%}
{%- endif -%}

{%- if product_title_lower contains 'shirt' or product_type_lower contains 'shirt' -%}
  {%- assign is_shirt = true -%}
{%- endif -%}

{%- if product_title_lower contains 'hat' or product_title_lower contains 'cap' or product_title_lower contains 'beanie' or product_type_lower contains 'headwear' or product_type_lower contains 'hat' -%}
  {%- assign is_headwear = true -%}
{%- endif -%}

{%- if product_title_lower contains 'footwear' or product_type_lower contains 'footwear' or product_title_lower contains 'clog' or product_type_lower contains 'clog' or product_title_lower contains 'shoe' or product_type_lower contains 'shoe' or product_title_lower contains 'boot' or product_type_lower contains 'boot' or product_title_lower contains 'sandal' or product_type_lower contains 'sandal' -%}
  {%- assign is_footwear = true -%}
{%- endif -%}

